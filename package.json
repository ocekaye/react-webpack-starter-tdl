  "scripts": {
    "build": "NODE_ENV=production npm run build:clean && webpack --config webpack.production.config.js --mode production --optimize-minimize  --colors --progress",
    "build-windows": "SET NODE_ENV=production && npm run build:clean && webpack --mode production --optimize-minimize --config webpack.production.config.js --colors --progress",
